body
  margin: 0;
  padding: 20px;
  font: 14px Helvetica, Arial, sans-serif;

a
  color: #00B7FF;
  text-decoration: none;

table
  margin: 10px 0;
  border: 2px solid #000;
  border-collapse: collapse;
  
  td
    width: 50px;
    height: 50px;
    font-size: 20px;
    text-align: center;
    border: 1px solid #000;

h1
  margin: 0
  padding: 0

.pink
  color: #FF00FF;

.blue
  color: #0000FF;

.you
  background: #FFB6C1;

.challenger
  background: #ADD8E6;

#game
  margin: 20px 0 0 0;
  width: 270px;

#score
  font-size: 18px;
  
#display
  font-size: 20px;
  text-align: center;